<div class="sample_wish_card p-4 m-2 w-sm">
      <div class="d-flex justify-content-between">
        <div>
          <h5 class="text-secondary"><%= iscomplete %></h5>
        </div>
        <!--スター-->
        <div class="sample-star-rate" data-score=<%= star_score %>></div>
      </div>

      <div class="d-flex flex-column  align-items-center text-align-center pt-2 mt-2">
        <div><%= image_tag  "#{image_name}" , size: "200x200", class: "rounded-circle" %></div>
        <div class="pt-3 text-center text-info"><%= title %></div>
        <div class="pt-1 text-center"><%= span %>までに達成予定</div>
      </div>

</div>
