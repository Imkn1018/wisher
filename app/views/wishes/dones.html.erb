dones
<% @tag_list.each do |list| %>
  <% if list.wishes.where(isCompleted:true).count != 0  %>
     <p><%=link_to list.tag_name,wishes_dones_path(tag_id:list.id)%><%= list.wishes.where(isCompleted:true).count %></p>
  <% end %>
<% end  %>

<% if params[:tag_id]  %>
  <h1><%= @tag.tag_name %>のタグ</h1>
<% end  %>
<table class="table table-bordered border-dark">
 <tbody>
<% @wishes.each do |wish| %>
  <tr>
    <td>
      <%= link_to wish_path(wish.id) do %>
        <%= attachment_image_tag wish, :wish_image , size:'200x200'%>
      <% end %>
    </td>
    <td class="derivery-info"><%= wish.wish_title %></td>
    <td class="derivery-info"><%= wish.memo %></td>
    <td class="text-center">
    </td>
  </tr>
<% end %>
</tbody>
</table>